SELECT * FROM LIZ_STORE; 

--what items from the store do we have more than 50?
SELECT * FROM LIZ_STORE where price <50 order by quantity;

--how many total items do we have in the store?
select sum(quantity) from LIZ_STORE;

--what is the total price of all items in the store?
select sum(price) from LIZ_STORE;

--what items from the store have more than 65 total and arrange by the highest price? 
SELECT * FROM LIZ_STORE WHERE QUANTITY >65 ORDER BY PRICE;
